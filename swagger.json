  "info": {
    "title": "Birthday Campaign",
    "description": "API description",
    "version": "1.0",
    "contact": {}
  },
  "tags": [],
  "servers": [],
  "components": {
    "securitySchemes": {
      "bearer": {
        "scheme": "bearer",
        "bearerFormat": "JWT",
        "type": "http"
      }
    },
    "schemas": {
      "CreateUserDto": {
        "type": "object",
        "properties": {
          "_id": {
            "type": "string",
            "description": "The unique identifier of the discount"
          },
          "name": {
            "type": "string",
            "description": "Name of the user"
          },
          "email": {
            "type": "string",
            "description": "Email of the user"
          },
          "password": {
            "type": "string",
            "description": "Password"
          },
          "birthdate": {
            "type": "string",
            "description": "Birthdate of the user",
            "example": "2025-02-12T00:00:00.000Z"
          },
          "preferences": {
            "type": "array",
            "description": "User preferences",
            "items": {
              "type": "string",
              "enum": [
                "vegetables",
                "dairy",
                "meat",
                "fruits",
                "Seafood",
                "Grains"
              ]
            }
          }
        },
        "required": [
          "_id",
          "name",
          "email",
          "password",
          "birthdate",
          "preferences"
        ]
      },
      "LoginDto": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "description": "User email",
            "example": "test@gmail.com"
          },
          "password": {
            "type": "string",
            "description": "User password",
            "example": "userPassword123"
          }
        },
        "required": [
          "email",
          "password"
        ]
      },
      "ProductDto": {
        "type": "object",
        "properties": {
          "_id": {
            "type": "string",
            "description": "The unique identifier of the discount"
          },
          "name": {
            "type": "string",
            "description": "The name of the product",
            "example": "Sample Product"
          },
          "category": {
            "type": "string",
            "description": "The category of the product",
            "example": "Electronics"
          },
          "rating": {
            "type": "number",
            "description": "The rating of the product, from 0 to 5",
            "example": 4
          }
        },
        "required": [
          "_id",
          "name",
          "category",
          "rating"
        ]
      }
    }
  }
}